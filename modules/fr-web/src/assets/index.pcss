
html {
    -webkit-tap-highlight-color: #0000;
}

body {
    margin: 0;
    padding: 0;
    font-family: -apple-system, "SF UI Text", "Helvetica Neue", Arial, "PingFang SC", "Hiragino Sans GB", "Microsoft YaHei", "WenQuanYi Zen Hei", sans-serif;
    overflow: hidden;
    font-weight: normal;
    line-height: 18px;
    -webkit-font-smoothing: antialiased;
}

body * {
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;

    margin: 0;
    padding: 0;
}

@media screen and (-webkit-min-device-pixel-ratio: 2), not all

::-webkit-scrollbar {
    width: 6px;
    height: 6px;
}

::-webkit-scrollbar-corner {
    background-color: #0000;
}

::-webkit-scrollbar-thumb {
    background-color: #DEDEE4;
    border-radius: 4px;
}

::-webkit-scrollbar-track {
    background-color: #0000;
}

a {
    text-decoration: none;
    color: #7d8695;
    cursor: pointer;
}

/*背景格子*/
.grid-background {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-image: url(data:image/svg+xml;base64,);
    background-size: 100px 100px, 100px 100px, 20px 20px, 20px 20px;
    background-position: -2px -2px;
    mix-blend-mode: difference;
}

._404 {
    height: 100vh;
    background: #f8f8f8;
    display: flex;
    justify-content: center;
    align-items: center;
//padding-top: 199px; flex-direction: column;
    font-size: 18px;
    color: #666;
}
